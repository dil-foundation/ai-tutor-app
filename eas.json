{
  "cli": {
    "version": ">= 16.7.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": false,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "android": {
        "gradleCommand": ":app:assembleDebug",
        "buildType": "apk"
      }
    },
    "development-expo-go": {
      "developmentClient": false,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "android": {
        "gradleCommand": ":app:assembleDebug",
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": false,
        "enterpriseProvisioning": "universal"
      },
      "android": {
        "gradleCommand": ":app:assembleRelease",
        "buildType": "apk",
        "env": {
          "EXPO_USE_KOTLIN_SYNTHETIC": "false"
        }
      }
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "simulator": false,
        "enterpriseProvisioning": "universal",
        "buildConfiguration": "Release"
      },
      "android": {
        "gradleCommand": ":app:bundleRelease",
        "buildType": "app-bundle"
      }
    },
    "app-store": {
      "extends": "production",
      "ios": {
        "simulator": false,
        "enterpriseProvisioning": "universal",
        "buildConfiguration": "Release"
      }
    },
    "play-store": {
      "extends": "production",
      "android": {
        "gradleCommand": ":app:bundleRelease",
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "your-apple-id@example.com",
        "ascAppId": "your-app-store-connect-app-id",
        "appleTeamId": "your-apple-team-id"
      },
      "android": {
        "serviceAccountKeyPath": "path/to/api-key.json",
        "track": "internal"
      }
    },
    "preview": {
      "ios": {
        "appleId": "your-apple-id@example.com",
        "ascAppId": "your-app-store-connect-app-id",
        "appleTeamId": "your-apple-team-id"
      },
      "android": {
        "serviceAccountKeyPath": "path/to/api-key.json",
        "track": "internal"
      }
    },
    "app-store": {
      "ios": {
        "appleId": "your-apple-id@example.com",
        "ascAppId": "your-app-store-connect-app-id",
        "appleTeamId": "your-apple-team-id",
        "releaseChannel": "production"
      }
    },
    "play-store": {
      "android": {
        "serviceAccountKeyPath": "path/to/api-key.json",
        "track": "production",
        "releaseStatus": "draft"
      }
    }
  }
}
