{
  "expo": {
    "name": "DIL -AI TUTOR",
    "slug": "dil-mobile-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ios-appicon.png",
    "scheme": "diltutorappreact",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "displayName": "DIL -AI TUTOR",
      "supportsTablet": true,
      "bundleIdentifier": "com.dilai.lms",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSMicrophoneUsageDescription": "This app uses the microphone to allow you to practice your speaking skills.",
        "NSCameraUsageDescription": "This app uses the camera for UXCam session recording to improve user experience.",
        "NSPhotoLibraryUsageDescription": "This app may access photo library for UXCam session recording."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ios-appicon.png",
        "backgroundColor": "#111629"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.dilai.lms",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE"
      ],
      "buildToolsVersion": "34.0.0",
      "compileSdkVersion": 34,
      "targetSdkVersion": 34,
      "minSdkVersion": 21
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/ios-appicon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#111629"
        }
      ],
      "expo-secure-store",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9a1d7eb1-4a6c-4662-9691-bf042019099d"
      }
    },
    "owner": "dil-tutor"
  }
}
