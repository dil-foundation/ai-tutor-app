{
  "name": "dil-tutor-app-e2e-tests",
  "version": "1.0.0",
  "description": "End-to-end tests for DIL Tutor App using Appium and BrowserStack App Automate",
  "main": "index.js",
  "scripts": {
    "test": "wdio run wdio.conf.js",
    "test:simple": "wdio run wdio.simple.conf.js",
    "test:minimal": "wdio run wdio.minimal.conf.js",
    "test:web": "wdio run wdio.web-test.conf.js",
    "test:automate": "wdio run wdio.browserstack-automate.conf.js",
    "test:session": "wdio run wdio.session-test.conf.js",
    "test:simple-session": "wdio run wdio.simple-session.conf.js",
    "test:device": "wdio run wdio.device-test.conf.js",
    "test:local": "wdio run wdio.local-test.conf.js",
    "test:direct": "wdio run wdio.direct-appium.conf.js",
    "test:android": "wdio run wdio.android.conf.js",
    "test:ios": "wdio run wdio.ios.conf.js",
    "test:browserstack": "wdio run wdio.browserstack.conf.js",
    "test:android-browserstack": "wdio run wdio.android-browserstack.conf.js",
    "test:ios-browserstack": "wdio run wdio.ios-browserstack.conf.js",
    "test:parallel": "wdio run wdio.parallel.conf.js",
    "test:smoke": "wdio run wdio.smoke.conf.js",
    "test:regression": "wdio run wdio.regression.conf.js",
    "test:debug": "wdio run wdio.debug.conf.js",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "clean": "rimraf logs screenshots videos",
    "setup": "node scripts/setup.js",
    "check-appium": "node scripts/check-appium.js"
  },
  "dependencies": {
    "@wdio/allure-reporter": "^8.32.0",
    "@wdio/appium-service": "^8.46.0",
    "@wdio/browserstack-service": "^8.47.0",
    "@wdio/cli": "^8.32.0",
    "@wdio/local-runner": "^8.32.0",
    "@wdio/mocha-framework": "^8.32.0",
    "@wdio/spec-reporter": "^8.32.0",
    "appium": "^2.2.1",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.1",
    "devtools": "^8.14.0",
    "dotenv": "^16.4.5",
    "moment": "^2.30.1",
    "rimraf": "^5.0.10",
    "uuid": "^9.0.1",
    "webdriverio": "^8.32.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.24",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "appium-uiautomator2-driver": "^2.19.0",
    "appium-xcuitest-driver": "^2.19.0",
    "eslint": "^8.57.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "appium",
    "webdriverio",
    "e2e",
    "testing",
    "mobile",
    "react-native",
    "expo",
    "browserstack"
